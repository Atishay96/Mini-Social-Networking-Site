  </head>
  <body>
  	<br/>
    <a href="./">Sign Up</a>&nbsp;
    <a href="#">Need Help?</a>&nbsp;&nbsp;
  <br/><br/><br/><br/>
  	<div class="container">
  	<center>
  	<div class="xyz">
  	<br/>
  	<form action="/login" method="POST">
		<div class="form-group">
			<div class="col-sm-7">
				<div class="rows">
					 <input type="text" id="email" name="email" class="form-control" style="width: 350px;" placeholder="Enter E-Mail Address" required/>
				</div>
			</div>
		</div>	
		<div class="form-group">
			<div class="col-sm-7"> <br/>
			  <input type="password" id="password" name="password" class="form-control" placeholder="Enter Password" style="width: 350px;" required/>
			</div>
		</div>
		<br/><br/><br/><br/>
		<div class="form-group">
			<div class="col-sm-8">
			  <input type="submit" id="submit" class="btn btn-md btn-danger" value="Log In">
			</div>
		</div>
		<br/><br/>
		
	</form>	
  	</center>
  </body>
